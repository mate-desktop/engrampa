msgstr ""

#: data/engrampa.appdata.xml.in.in:24
msgid "ARJ Compressed Archive (.arj)"
msgstr ""

#: data/engrampa.appdata.xml.in.in:25
msgid "Cabinet File (.cab)"
msgstr ""

#: data/engrampa.appdata.xml.in.in:26
