#: data/engrampa.appdata.xml.in:26
msgid "UNIX CPIO Archive (.cpio)"
msgstr ""

#: data/engrampa.appdata.xml.in:27
msgid "Debian Package (.deb, .udeb) [Read-only mode]"
msgstr ""

